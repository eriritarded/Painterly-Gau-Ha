{
  "multipart": [
    {"apply": {"model": "quarryplus:frame_post"}},
    {
      "when": {"north": "true"},
      "apply": {"model": "quarryplus:frame_side", "uvlock": true}
    },
    {
      "when": {"east": "true"},
      "apply": {"model": "quarryplus:frame_side", "y": 90, "uvlock": true}
    },
    {
      "when": {"south": "true"},
      "apply": {"model": "quarryplus:frame_side", "y": 180, "uvlock": true}
    },
    {
      "when": {"west": "true"},
      "apply": {"model": "quarryplus:frame_side", "y": 270, "uvlock": true}
    },
    {
      "when": {"top": "true"},
      "apply": {"model": "quarryplus:frame_side", "x": 270, "uvlock": true}
    },
    {
      "when": {"down": "true"},
      "apply": {"model": "quarryplus:frame_side", "x": 90, "uvlock": true}
    }
  ]
}
